{
    "version":2,
    "builds" :[
        {
            "src":"project/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"34mb","runtime":"python3.7.3"}

        },
        {
            "src":"project/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "distDir" :"staticfiles_build"
            }  
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"project/wsgi.py"
        }
    ]
}